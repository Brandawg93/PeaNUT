{
  "name": "peanut",
  "version": "4.4.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next .swc coverage test-results",
    "dev": "docker compose up -d && next dev --turbo && docker compose down",
    "build": "next build",
    "build:local": "npm run build && cp -r .next/static .next/standalone/.next/",
    "build:analyze": "ANALYZE=true next build",
    "start": "HOSTNAME=$WEB_HOST PORT=$WEB_PORT node server.js",
    "start:local": "node .next/standalone/server.js",
    "type-check": "tsc --noEmit",
    "lint": "next lint --dir .",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "precheck": "concurrently \"npm run type-check\" \"lint-staged\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test && docker compose down",
    "test:all": "concurrently \"npm run test\" \"npm run test:e2e\"",
    "deploy": "./scripts/deploy.sh",
    "deploy:test": "./scripts/deploy-test.sh",
    "telemetry": "next telemetry disable",
    "prepare": "husky"
  },
  "dependencies": {
    "@codemirror/lang-yaml": "^6.1.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@influxdata/influxdb-client": "^1.35.0",
    "@influxdata/influxdb-client-apis": "^1.35.0",
    "@material-tailwind/react": "^2.1.10",
    "@mui/material": "^6.3.0",
    "@mui/x-charts": "^7.23.2",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-table": "^8.20.6",
    "@uiw/codemirror-theme-vscode": "^4.23.7",
    "@uiw/react-codemirror": "^4.23.7",
    "chokidar": "^4.0.3",
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-resources-to-backend": "^1.2.1",
    "js-yaml": "^4.1.0",
    "ldrs": "^1.0.2",
    "next": "^15.1.3",
    "next-remove-imports": "^1.0.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.4.0",
    "react-toastify": "^11.0.2",
    "toad-scheduler": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@next/bundle-analyzer": "^15.1.3",
    "@next/eslint-plugin-next": "^15.1.3",
    "@playwright/test": "^1.49.1",
    "@tanstack/eslint-plugin-query": "^5.62.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/swagger-ui-react": "^4.18.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "next-swagger-doc": "^0.4.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "swagger-ui-react": "^5.18.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "lint-staged": {
    "*": "eslint"
  }
}
