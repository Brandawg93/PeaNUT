{
  "name": "nut-dashboard",
  "version": "1.3.0",
  "description": "Dashboard for Network UPS Tools",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc && npm run build --prefix client",
    "format": "prettier --write **/*.ts*",
    "lint": "eslint **/*.ts* --fix",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "concurrently \"tsc --watch\" \"nodemon server/**/*\"",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brandawg93/PeaNUT.git"
  },
  "keywords": [
    "reddit",
    "bots"
  ],
  "author": "brandawg93",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Brandawg93/PeaNUT/issues"
  },
  "homepage": "https://github.com/Brandawg93/PeaNUT#readme",
  "dependencies": {
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "graphql-http": "^1.22.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/cors": "~2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
