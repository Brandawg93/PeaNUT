    "clsx": "^2.1.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-resources-to-backend": "^1.2.1",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.488.0",
    "next": "^15.3.0",
    "next-auth": "~5.0.0-beta.25",
    "next-remove-imports": "^1.0.12",
    "next-themes": "^0.4.6",
    "next-ws": "^2.0.6",
    "react": "^19.1.0",
