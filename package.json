{
  "name": "peanut",
  "version": "2.6.2",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next .swc coverage test-results",
    "dev": "docker compose up -d && next dev && docker compose down",
    "build": "next build",
    "build:local": "npm run build && cp -r .next/static .next/standalone/.next/",
    "build:analyze": "ANALYZE=true next build",
    "start": "HOSTNAME=$WEB_HOST PORT=$WEB_PORT node server.js",
    "start:local": "node .next/standalone/server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "precheck": "concurrently \"npm run type-check\" \"npm run lint\" \"npm run format\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "docker compose up -d && playwright test && docker compose down",
    "test:all": "concurrently \"npm run test\" \"npm run test:e2e\"",
    "deploy": "./scripts/deploy.sh",
    "deploy:test": "./scripts/deploy-test.sh",
    "telemetry": "next telemetry disable",
    "prepare": "husky"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@influxdata/influxdb-client": "^1.35.0",
    "@material-tailwind/react": "^2.1.10",
    "@tanstack/react-query": "^5.56.2",
    "chart.js": "^4.4.3",
    "chartjs-plugin-annotation": "^3.0.1",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-resources-to-backend": "^1.2.1",
    "js-yaml": "^4.1.0",
    "ldrs": "^1.0.2",
    "next": "^14.2.9",
    "next-i18next": "^15.3.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-toastify": "^10.0.5",
    "tailwindcss-animated": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.10.0",
    "@next/bundle-analyzer": "^14.2.9",
    "@playwright/test": "^1.45.1",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.9",
    "@types/react": "18.2.42",
    "@types/react-dom": "^18.3.0",
    "@types/swagger-ui-react": "^4.18.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-swagger-doc": "^0.4.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "swagger-ui-react": "^5.17.14",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.5.0"
  }
}
