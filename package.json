    "lucide-react": "^0.475.0",
    "next": "^15.1.7",
    "next-auth": "5.0.0-beta.25",
    "next-remove-imports": "^1.0.12",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.1",
