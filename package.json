{
  "name": "peanut",
  "version": "5.19.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next .swc coverage test-results",
    "dev": "docker compose up -d && (next dev || true) && docker compose down",
    "dev:next": "next dev",
    "build": "next build",
    "build:local": "npm run build && cp -r .next/static .next/standalone/.next/",
    "build:analyze": "ANALYZE=true next build",
    "start": "node server.js",
    "start:local": "node .next/standalone/server.js",
    "type-check": "next typegen && tsc --noEmit",
    "lint": "eslint --cache",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "format": "prettier --check . --cache",
    "format:fix": "prettier --write . --cache",
    "precheck": "concurrently \"npm run type-check\" \"lint-staged\"",
    "fullcheck": "concurrently \"npm run lint\" \"npm run stylelint\" \"npm run type-check\" \"npm run format\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "docker compose up -d && playwright test && WEB_USERNAME=admin WEB_PASSWORD=test123 playwright test && docker compose down",
    "test:all": "concurrently \"npm run test\" \"npm run test:e2e\"",
    "deploy:local": "./deploy.sh local",
    "deploy:test": "./deploy.sh test",
    "deploy:production": "./deploy.sh production",
    "telemetry": "next telemetry disable",
    "next-ws": "next-ws patch --yes",
    "prepare": "husky && npm run next-ws"
  },
  "dependencies": {
    "@codemirror/lang-yaml": "^6.1.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
